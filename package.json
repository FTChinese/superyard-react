{
  "name": "superyard-react",
  "version": "1.1.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "jest",
    "postversion": "git push && git push --tags",
    "html-dev": "ts-node --project=scripts/tsconfig.json ./scripts/html.dev.ts",
    "html": "ts-node --project=scripts/tsconfig.json ./scripts/html.prod.ts",
    "copy-static": "ts-node --project=scripts/tsconfig.json ./scripts/copyStaticAssets.ts",
    "deploy-dev": "yarn run build && yarn run html && yarn run copy-static",
    "deploy": "yarn run build && yarn run html && yarn run copy-static --prod"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "date-fns": "^2.28.0",
    "formik": "^2.2.9",
    "react": "^17.0.0",
    "react-bootstrap": "^2.2.3",
    "react-dom": "^17.0.0",
    "react-router-dom": "^6.3.0",
    "react-toastify": "^8.1.0",
    "recoil": "^0.7.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/jsdom": "^16.2.14",
    "@types/nunjucks": "^3.2.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/shelljs": "^0.8.11",
    "@vitejs/plugin-react": "^1.3.1",
    "bootstrap": "^5.1.3",
    "jest": "^27.5.1",
    "nunjucks": "^3.2.3",
    "prettier": "2.6.2",
    "shelljs": "^0.8.5",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.4.0",
    "typescript": "^4.3.2",
    "vite": "^2.6.4",
    "vite-plugin-svgr": "^0.6.0"
  }
}
