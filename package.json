{
  "name": "superyard-react",
  "version": "1.5.1",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "jest",
    "postversion": "git push && git push --tags",
    "html-dev": "ts-node --project=scripts/tsconfig.json ./scripts/html.dev.ts",
    "html": "ts-node --project=scripts/tsconfig.json ./scripts/html.prod.ts",
    "copy-static": "ts-node --project=scripts/tsconfig.json ./scripts/copyStaticAssets.ts",
    "deploy-dev": "yarn run build && yarn run html && yarn run copy-static",
    "deploy": "yarn run build && yarn run html && yarn run copy-static --prod"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "date-fns": "^2.29.1",
    "formik": "^2.2.9",
    "react": "^18.2.0",
    "react-bootstrap": "^2.4.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "react-toastify": "^9.0.7",
    "recoil": "^0.7.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/jsdom": "^20.0.0",
    "@types/nunjucks": "^3.2.1",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/shelljs": "^0.8.11",
    "@vitejs/plugin-react": "^2.0.0",
    "bootstrap": "^5.2.0",
    "jest": "^28.1.3",
    "nunjucks": "^3.2.3",
    "shelljs": "^0.8.5",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.3"
  },
  "resolutions": {
    "@types/react": "^18.0.0"
  }
}
